#include <bits/stdc++.h>
using namespace std;
#define ll long long int
int main() {
	ll i,j,t,n,k,l;
	const unsigned ll M = 1000000007; 
	cin>>t;
	vector<ll>g(102);
	g[0]=1;
    g[1]=1;
    for(i=2;i<=101;i++)
    {
        g[i]=(g[i-1]+g[i-2])%M;
    }
	while(t--)
	{
	    cin>>l;
	    cout<<g[l+1]<<"\n";
	}
	return 0;
}

**link https://practice.geeksforgeeks.org/problems/consecutive-1s-not-allowed/0
